        </inbodydescription>
        <location file="api/src/main/java/org/apache/iceberg/expressions/StrictMetricsEvaluator.java" line="75" column="18" bodyfile="api/src/main/java/org/apache/iceberg/expressions/StrictMetricsEvaluator.java" bodystart="75" bodyend="78"/>
      </memberdef>
